{
  "name": "stata-exec",
  "main": "./lib/stata-exec",
  "version": "0.0.1",
  "description": "Send Stata code to the Stata GUI window",
  "activationCommands": {
    "atom-text-editor": [
      "r-exec:send-command",
      "r-exec:send-function",
      "r-exec:send-paragraph",
      "r-exec:send-knitr",
      "r-exec:setwd",
      "r-exec:set-chrome",
      "r-exec:set-iterm",
      "r-exec:set-iterm2",
      "r-exec:set-rapp",
      "r-exec:set-rstudio",
      "r-exec:set-safari",
      "r-exec:set-terminal",
      "r-exec:insert-assignment",
      "r-exec:insert-pipe"
    ]
  },
  "repository": "https://github.com/kylebarron/atom-r-exec",
  "keywords": [
    "Stata",
    "code",
    "run",
    "runner"
  ],
  "license": "MIT",
  "engines": {
    "atom": ">1.0.0"
  },
  "dependencies": {
    "node-osascript": "1.0.4",
    "path": "0.12.7"
  }
}
