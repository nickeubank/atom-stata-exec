{
  "name": "stata-exec",
  "main": "./lib/stata-exec",
  "version": "0.2.1",
  "description": "Send Stata code to the Stata GUI window",
  "activationCommands": {
    "atom-text-editor": [
      "stata-exec:send-command",
      "stata-exec:send-function",
      "stata-exec:send-paragraph",
      "stata-exec:setwd"
    ]
  },
  "repository": "https://github.com/nickeubank/atom-stata-exec",
  "keywords": [
    "Stata",
    "code",
    "run",
    "runner"
  ],
  "license": "MIT",
  "engines": {
    "atom": ">1.0.0"
  },
  "dependencies": {
    "node-osascript": "1.0.4",
    "path": "0.12.7"
  }
}
